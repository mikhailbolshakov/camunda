{
  "subscribers": [
    {
      "topic": "camunda.service-task.rgs.med.client-task-feedback.get-client",
      "handler": {
        "type": "serviceTask",
        "delay": 2,
        "actions": [
          {
            "type": "publish",
            "topic": "camunda.service-task.completion",
            "copySourceVariables": true,
            "message": {
              "header": {
                "messageId": "123",
                "schemaVersion": "1.0.0",
                "timestamp": "2020-01-01T18:30:00.000Z"
              },
              "completionContext": {
                "taskId": "#{context.sourceMessage.taskContext.taskId}"
              },
              "variables": {
                "feedback": {
                  "isRgsClient": true
                }
              }
            }
          }
        ]
      }
    },
    {
      "topic": "camunda.service-task.rgs.med.client-task-feedback.delete-os",
      "handler": {
        "type": "serviceTask",
        "delay": 2,
        "actions": [
          {
            "type": "publish",
            "topic": "camunda.service-task.completion",
            "copySourceVariables": true,
            "message": {
              "header": {
                "messageId": "123",
                "schemaVersion": "1.0.0",
                "timestamp": "2020-01-01T18:30:00.000Z"
              },
              "completionContext": {
                "taskId": "#{context.sourceMessage.taskContext.taskId}"
              },
              "variables": {
                "feedback": {
                  "osProvided": false
                }
              }
            }
          }
        ]
      }
    }
  ]
}
