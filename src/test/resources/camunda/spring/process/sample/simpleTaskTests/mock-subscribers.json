{
  "subscribers": [
    {
      "topic": "service-task.service-task.test-task",
      "handler": {
        "type": "serviceTask",
        "delay": 2,
        "actions": [
          {
            "type": "publish",
            "topic": "service-task.completion",
            "copySourceVariables": true,
            "message": {
              "header": {
                "messageId": "123",
                "schemaVersion": "1.0.0",
                "timestamp": "2020-01-01T18:30:00.000Z"
              },
              "completionContext": {
                "taskId": "#{context.sourceMessage.taskContext.taskId}"
              },
              "variables": {
                "onlineService": {
                  "callTime": 20,
                  "problemSolved": "#{context.random.nextBoolean()}"
                }
              }
            }
          }
        ]
      }
    },
    {
      "topic": "user-task.user-task-process.test-task",
      "handler": {
        "type": "userTask",
        "delay": 2,
        "actions": [
          {
            "type": "publish",
            "topic": "user-task.completion",
            "copySourceVariables": true,
            "message": {
              "header": {
                "messageId": "123",
                "schemaVersion": "1.0.0",
                "timestamp": "2020-01-01T18:30:00.000Z"
              },
              "completionContext": {
                "taskId": "#{context.sourceMessage.taskContext.taskId}"
              },
              "variables": {
                "onlineService": {
                  "callTime": 20,
                  "problemSolved": "#{context.random.nextBoolean()}"
                }
              }
            }
          }
        ]
      }
    }
  ]
}