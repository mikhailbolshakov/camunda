{
  "subscribers": [
    {
      "topic": "test.s-task",
      "handler": {
        "type": "serviceTask",
        "delay": 2,
        "actions": [
          {
            "type": "publish",
            "topic": "service-task.completion",
            "copySourceVariables": true,
            "message": {
              "completionContext": {
                "taskId": "#{context.sourceMessage.taskContext.taskId}"
              },
              "variables": {
                "onlineService": {
                  "callTime": 20,
                  "problemSolved": "#{context.random.nextBoolean()}"
                }
              }
            }
          }
        ]
      }
    },
    {
      "topic": "test.u-task",
      "handler": {
        "type": "userTask",
        "delay": 2,
        "actions": [
          {
            "type": "publish",
            "topic": "user-task.completion",
            "copySourceVariables": true,
            "message": {
              "completionContext": {
                "taskId": "#{context.sourceMessage.taskContext.taskId}"
              },
              "variables": {
                "onlineService": {
                  "callTime": 20,
                  "problemSolved": "#{context.random.nextBoolean()}"
                }
              }
            }
          }
        ]
      }
    }
  ]
}